"dh_installcron\n"
"dh_installdebconf\n"
"dh_installemacsen\n"
"dh_installifupdown\n"
"dh_installinfo\n"
"dh_installinit\n"
"dh_installmenu\n"
"dh_installmime\n"
"dh_installmodules\n"
"dh_installlogcheck\n"
"dh_installlogrotate\n"
"dh_installpam\n"
"dh_installppp\n"
"dh_installudev\n"
"dh_installwm\n"
"dh_installxfonts\n"
"dh_bugfiles\n"
